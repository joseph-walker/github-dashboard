{
  "name": "github-dashboard",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "gql-codegen": "graphql-codegen -r dotenv/config --config codegen.yml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/urql-introspection": "2.1.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-svelte-csf": "^2.0.3",
    "@storybook/svelte": "^6.4.22",
    "@sveltejs/adapter-netlify": "^1.0.0-next.51",
    "@sveltejs/kit": "^1.0.0-next.299",
    "babel-loader": "^8.2.5",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.4.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.4",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.0.0",
    "typescript": "^4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-graphcache": "^4.3.6",
    "@urql/svelte": "^1.3.3",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "fp-ts": "^2.11.9",
    "graphql": "^15.8.0",
    "monocle-ts": "^2.3.13",
    "svelte-kit-cookie-session": "^2.1.2"
  }
}
